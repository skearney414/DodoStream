{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "distribution": "internal",
      "channel": "base"
    },
    "development": {
      "extends": "base",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      },
      "env": {
        "APP_VARIANT": "dev"
      },
      "environment": "development",
      "channel": "development"
    },
    "development_tv": {
      "extends": "development",
      "env": {
        "APP_VARIANT": "dev",
        "EXPO_TV": "1"
      },
      "environment": "development",
      "channel": "development_tv"
    },
    "production": {
      "extends": "base",
      "environment": "production",
      "channel": "production",
      "env": {
        "APP_VARIANT": "prod"
      },
      "autoIncrement": true
    },
    "production_apk": {
      "extends": "production",
      "android": {
        "buildType": "apk"
      }
    },
    "production_tv": {
      "extends": "production",
      "autoIncrement": true,
      "env": {
        "APP_VARIANT": "prod",
        "EXPO_TV": "1"
      },
      "environment": "production",
      "channel": "production_tv"
    },
    "production_tv_apk": {
      "extends": "production_tv",
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}